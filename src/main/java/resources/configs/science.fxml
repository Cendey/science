<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TreeTableColumn?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="971.0"
      xmlns="http://javafx.com/javafx/8.0.125" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.netex.apps.ctrl.Controller">
    <children>
        <MenuBar VBox.vgrow="NEVER">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" text="New"/>
                        <MenuItem mnemonicParsing="false" text="Open…"/>
                        <Menu mnemonicParsing="false" text="Open Recent"/>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                        <MenuItem mnemonicParsing="false" text="Close"/>
                        <MenuItem mnemonicParsing="false" text="Save"/>
                        <MenuItem mnemonicParsing="false" text="Save As…"/>
                        <MenuItem mnemonicParsing="false" text="Revert"/>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                        <MenuItem mnemonicParsing="false" text="Preferences…"/>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                        <MenuItem fx:id="miExit" mnemonicParsing="false" onAction="#exitApp" text="Quit"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Undo"/>
                        <MenuItem mnemonicParsing="false" text="Redo"/>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                        <MenuItem mnemonicParsing="false" text="Cut"/>
                        <MenuItem mnemonicParsing="false" text="Copy"/>
                        <MenuItem mnemonicParsing="false" text="Paste"/>
                        <MenuItem mnemonicParsing="false" text="Delete"/>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                        <MenuItem mnemonicParsing="false" text="Select All"/>
                        <MenuItem mnemonicParsing="false" text="Unselect All"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About File Conversion"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <GridPane prefHeight="1200.0" prefWidth="700.0" VBox.vgrow="ALWAYS">
            <columnConstraints>
                <ColumnConstraints hgrow="NEVER" minWidth="10.0" prefWidth="120.0"/>
                <ColumnConstraints hgrow="NEVER" minWidth="10.0" prefWidth="120.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="120.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="120.0"/>
                <ColumnConstraints hgrow="NEVER" maxWidth="120.0" minWidth="10.0" prefWidth="120.0"/>
                <ColumnConstraints hgrow="NEVER" minWidth="10.0" prefWidth="120.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="80.0" minHeight="10.0" prefHeight="50.0" vgrow="NEVER"/>
                <RowConstraints maxHeight="25.0" minHeight="10.0" prefHeight="25.0" vgrow="NEVER"/>
                <RowConstraints maxHeight="25.0" minHeight="10.0" prefHeight="25.0" vgrow="NEVER"/>
                <RowConstraints maxHeight="25.0" minHeight="10.0" prefHeight="25.0" vgrow="NEVER"/>
                <RowConstraints maxHeight="25.0" minHeight="10.0" prefHeight="25.0" vgrow="NEVER"/>
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="25.0" minHeight="10.0" prefHeight="25.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label fx:id="lblSrcFile" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                       prefHeight="25.0" prefWidth="77.0" style="&#10;" text="Source File" GridPane.rowIndex="1">
                    <font>
                        <Font name="Arial Bold" size="12.0"/>
                    </font>
                </Label>
                <TextField fx:id="srcPath" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                           prefHeight="25.0" prefWidth="453.0" style="-fx-alignment: CENTER-LEFT;"
                           GridPane.columnIndex="1" GridPane.columnSpan="4" GridPane.rowIndex="1"/>
                <Button fx:id="btnSrcPath" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                        mnemonicParsing="false" onAction="#chooseSrcPath" prefHeight="25.0" prefWidth="61.0" text="..."
                        GridPane.columnIndex="5" GridPane.rowIndex="1"/>
                <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="27.0"
                       prefWidth="77.0" text="Target File" GridPane.rowIndex="3">
                    <font>
                        <Font name="Arial Bold" size="12.0"/>
                    </font>
                </Label>
                <TextField fx:id="destPath" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                           prefHeight="25.0" prefWidth="453.0" style="-fx-alignment: CENTER-LEFT;"
                           GridPane.columnIndex="1" GridPane.columnSpan="4" GridPane.rowIndex="3"/>
                <Button fx:id="btnDestPath" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                        mnemonicParsing="false" onAction="#chooseDestPath" prefHeight="25.0" prefWidth="61.0" text="..."
                        GridPane.columnIndex="5" GridPane.rowIndex="3"/>
                <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="25.0"
                       prefWidth="84.0" text="Target Format" GridPane.columnIndex="4" GridPane.rowIndex="4">
                    <font>
                        <Font name="Arial Bold" size="12.0"/>
                    </font>
                </Label>
                <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="25.0"
                       prefWidth="77.0" text="Named As" GridPane.rowIndex="2">
                    <font>
                        <Font name="Arial Bold" size="12.0"/>
                    </font>
                </Label>
                <TextField fx:id="txtFuzzySrcFileName" maxHeight="1.7976931348623157E308"
                           maxWidth="1.7976931348623157E308" onMouseClicked="#chooseSrcFuzzyName" prefHeight="25.0"
                           prefWidth="281.0" style="-fx-alignment: CENTER-LEFT;" GridPane.columnIndex="1"
                           GridPane.columnSpan="3" GridPane.rowIndex="2"/>
                <Button fx:id="btnStart" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                        mnemonicParsing="false" onAction="#startWork" prefHeight="25.0" prefWidth="61.0" text="Start"
                        GridPane.columnIndex="4" GridPane.rowIndex="9">
                    <font>
                        <Font name="Arial Bold" size="12.0"/>
                    </font>
                </Button>
                <Button fx:id="btnCancel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                        mnemonicParsing="false" onAction="#cancelWork" prefHeight="25.0" prefWidth="61.0" text="Cancel"
                        GridPane.columnIndex="5" GridPane.rowIndex="9">
                    <font>
                        <Font name="Arial" size="12.0"/>
                    </font>
                </Button>
                <ProgressBar fx:id="progressIndicator" disable="true" maxHeight="1.7976931348623157E308"
                             maxWidth="1.7976931348623157E308" prefWidth="200.0" progress="0.0"
                             style="-fx-background-color: #fbcce7;" GridPane.columnSpan="2" GridPane.rowIndex="9"/>
                <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="25.0"
                       prefWidth="77.0" text="Renamed As" GridPane.rowIndex="4">
                    <font>
                        <Font name="Arial Bold" size="12.0"/>
                    </font>
                </Label>
                <CheckBox fx:id="cbxIndicatorForBatch" maxHeight="1.7976931348623157E308"
                          maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#validate"
                          prefHeight="25.0" prefWidth="62.0" style="-fx-background-color: #efdecd;" text="Batch?"
                          GridPane.columnIndex="5" GridPane.rowIndex="2">
                    <font>
                        <Font name="Arial Bold" size="12.0"/>
                    </font>
                </CheckBox>
                <TextField fx:id="txtDestPrefixName" maxHeight="1.7976931348623157E308"
                           maxWidth="1.7976931348623157E308" onMouseClicked="#chooseDestPrefixName" prefHeight="25.0"
                           prefWidth="281.0" style="-fx-alignment: CENTER-LEFT;" GridPane.columnIndex="1"
                           GridPane.columnSpan="3" GridPane.rowIndex="4"/>
                <CheckBox fx:id="cbxNeedFileHeader" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                          mnemonicParsing="false" prefHeight="25.0" prefWidth="70.0"
                          style="-fx-background-color: #efdecd;" text="Header?" GridPane.columnIndex="4"
                          GridPane.rowIndex="2">
                    <font>
                        <Font name="Arial Bold" size="12.0"/>
                    </font>
                </CheckBox>
                <ChoiceBox fx:id="cboDestFileFormat" maxHeight="1.7976931348623157E308"
                           maxWidth="1.7976931348623157E308" prefHeight="25.0" prefWidth="241.0"
                           style="-fx-background-color: #efdecd; -fx-text-fill: #ace5ee; -fx-font-size: 8pt;"
                           GridPane.columnIndex="5" GridPane.hgrow="NEVER" GridPane.rowIndex="4"
                           GridPane.vgrow="NEVER"/>
                <ImageView fx:id="appLogo" fitHeight="50.0" fitWidth="235.0" pickOnBounds="true" preserveRatio="true"
                           style="-fx-image-alignment: CENTER;" GridPane.columnIndex="2" GridPane.columnSpan="2"
                           GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.vgrow="NEVER"/>
                <TreeTableView fx:id="logTreeViewer" onMouseClicked="#openDirectoryOrFile" prefHeight="200.0"
                               prefWidth="200.0" GridPane.columnSpan="6" GridPane.rowIndex="5" GridPane.rowSpan="4">
                    <columns>
                        <TreeTableColumn fx:id="nameColumn" prefWidth="638.0" text="Name"/>
                        <TreeTableColumn fx:id="sizeColumn" minWidth="0.0" prefWidth="92.0" text="Size (KB)"/>
                        <TreeTableColumn fx:id="modifiedColumn" prefWidth="182.0" text="Date modified"/>
                        <TreeTableColumn fx:id="typeColumn" prefWidth="57.0" text="Type"/>
                    </columns>
                </TreeTableView>
            </children>
        </GridPane>
    </children>
</VBox>
